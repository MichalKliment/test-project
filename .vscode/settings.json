{
  "vscode-graphql.useSchemaFileDefinitions": true,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/hrapka": false,
    "**/appthor": false,
    "**/ralba": false,
    "**/tpa": false,
    "**/nft*": false,
    "**/invoicing*": false,
    "**/utulek": false,
    "**/invbuild": false,
    "**/pivobasilico": false,
    "**/pravni-sluzby": false,
    "**/libs": false,
    "**/dist": false,
    "**/hub*": false
  },
  "explorerExclude.backup": null,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  //   "liveSassCompile.settings.includeItems": [
  //     "keycloak/themes/ralba-theme/login/resources/css/main.scss",
  //     "keycloak/themes/ralba-theme/account/resources/css/main.scss"
  //   ],

  "cSpell.language": "en,cs",
